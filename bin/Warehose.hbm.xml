<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.wgl.supermarket.domain.Warehose" table="tb_warehose">
		<id name="id" column="id" type="java.lang.Integer" length="10">
			<generator class="increment"/>
		</id>
		<property name="name" column="name" type="java.lang.String" length="20"/>
		<property name="warehousetype" column="warehousetype" type="java.lang.Integer" length="4"/>
	</class>
</hibernate-mapping>