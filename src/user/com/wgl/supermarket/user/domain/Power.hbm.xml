<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.wgl.supermarket.domain.Power" table="tb_power">
		<id name="id" column="id" type="java.lang.Integer" length="10">
			<generator class="increment"/>
		</id>
		<property name="powername" column="powername" type="java.lang.String" length="20"/>
		<property name="powertype" column="powertype" type="java.lang.Integer" length="2"/>
	</class>
</hibernate-mapping>