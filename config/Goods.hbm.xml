<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.wgl.supermarket.domain.Goods" table="tb_goods">
		<id name="id" column="id" type="java.lang.Integer" length="10">
			<generator class="increment"/>
		</id>
		<property name="goodsname" column="goodsname" type="java.lang.String" length="50"/>
		<property name="produceplace" column="produceplace" type="java.lang.String" length="50"/>
		<property name="size" column="size" type="java.lang.String" length="50"/>
		<property name="goodspackage" column="goodspackage" type="java.lang.String" length="50"/>
		<property name="productcode" column="productcode" type="java.lang.String" length="20"/>
		<property name="promitcode" column="promitcode" type="java.lang.String" length="20"/>
		<property name="available" column="available" type="java.lang.Integer" length="4"/>
		<property name="shelflife" column="shelflife" type="java.lang.Integer" length="10"/>
		<property name="shelflifeunit" column="shelflifeunit" type="java.lang.String" length="10"/>
		<property name="barcode" column="barcode" type="java.lang.String" length="50"/>
		<property name="description" column="description" type="java.lang.String" length="50"/>
		<property name="providerid" column="providerid" type="java.lang.Integer" length="10"/>
		<property name="categoryid" column="categoryid" type="java.lang.Integer" length="10"/>
	</class>
</hibernate-mapping>