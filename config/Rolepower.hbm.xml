<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.wgl.supermarket.domain.Rolepower" table="tb_role_power">
		<id name="id" column="id" type="java.lang.Integer" length="10">
			<generator class="increment"/>
		</id>
		<property name="roleid" column="roleid" type="java.lang.Integer" length="10"/>
		<property name="powerid" column="powerid" type="java.lang.Integer" length="10"/>
	</class>
</hibernate-mapping>