<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.wgl.supermarket.domain.Purchasebillinfo" table="tb_purchasebillinfo">
		<id name="id" column="id" type="java.lang.Integer" length="10">
			<generator class="increment"/>
		</id>
		<property name="paytype" column="paytype" type="java.lang.String" length="50"/>
		<property name="inporttime" column="inporttime" type="java.lang.Date"/>
		<property name="operateperson" column="operateperson" type="java.lang.String" length="20"/>
		<property name="number" column="number" type="java.lang.Integer" length="10"/>
		<property name="comment" column="comment" type="java.lang.String" length="100"/>
		<property name="billtype" column="billtype" type="java.lang.Integer" length="4"/>
		<property name="providerid" column="providerid" type="java.lang.Integer" length="10"/>
		<property name="warehouseid" column="warehouseid" type="java.lang.Integer" length="10"/>
		<property name="userid" column="userid" type="java.lang.Integer" length="10"/>
		<property name="goodsid" column="goodsid" type="java.lang.Integer" length="10"/>
	</class>
</hibernate-mapping>